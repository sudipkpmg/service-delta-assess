</xsd:complexType>
<xsd:complexType name="EntityType">
    <xsd:annotation>
        <xsd:documentation>Entity data provided to and returned from an Assess action. Used to group all entity instances for a single entity type.
Note: Entities which are inferred by rules in the policy model must not be supplied in an assess request.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
        <xsd:element name="instance" type="EntityInstanceType" minOccurs="0" maxOccurs="unbounded">
