            <xsd:documentation>The name of the attribute for this decision report node</xsd:documentation>
        </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="type" type="AttributeTypeEnum" use="required">
        <xsd:annotation>
            <xsd:documentation>The data type of the attribute for this decision report node</xsd:documentation>
        </xsd:annotation>
    </xsd:attribute>
    <!--node attribute current text -->
    <xsd:attribute name="text" type="xsd:string" use="required">
        <xsd:annotation>
            <xsd:documentation>The text of the decision report for this node. For example "The child's serves of vegetables per day is 3." or "The child does not live at home."</xsd:documentation>
        </xsd:annotation>
    </xsd:attribute>
    <!--true if the attribute is inferred -->
    <xsd:attribute name="inferred" type="xsd:boolean" use="required">
